            <nav id="menu">
                {{ if eq .User nil }}
                <a hx-get="/login"
                   hx-target="#header-content"
                   hx-swap="outerhtml"
                   hx-select="#header-content"
                   hx-replace-url="true">
                    <img src="/static/media/logging.svg" alt="logging">
                    <p>Log in</p>
                </a>
            {{ else }}
                <a hx-get="/learn"
                   hx-target="main"
                   hx-swap="innerHTML"
                   hx-select="main"
                   hx-replace-url="true"
                   hx-on::after-request="collapseHeader()">
                    <img src="/static/media/learn.svg" alt="learn">
                    <p>Learn</p>
                </a>
                <a hx-get="/curriculum-modification"
                   hx-target="main"
                   hx-swap="innerHTML"
                   hx-select="main"
                   hx-replace-url="true"
                   hx-on::after-request="collapseHeader()">
                    <img src="/static/media/curriculum_modification.svg"
                         alt="curriculum modification">
                    <p>
                        Curriculum
                        <br>
                        modification
                    </p>
                </a>
                <a href="/logout">
                    <img src="/static/media/logging.svg" alt="logging">
                    <p>Log out</p>
                </a>
                {{ end }}
                <a hx-get="/settings"
                   hx-target="main"
                   hx-swap="innerHTML"
                   hx-select="main"
                   hx-replace-url="true"
                   hx-on::after-request="collapseHeader()">
                    <img src="/static/media/settings.svg" alt="settings">
                    <p>Settings</p>
                </a>
            </nav>
            <div id="header-content">{{ block "header" . }}{{ end }}</div>
        </header>
        <main>
            {{ block "main" . }}{{ end }}
        </main>
